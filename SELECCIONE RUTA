~Nodo(ALT = 2):  ; Equipo: PERU
  H.I(ALT = 1)  ; Equipo: PARAGUAY   ||   H.D(ALT = 1)  ; Equipo: URUGUAY

~Nodo(ALT = 1):  ; Equipo: PARAGUAY
  H.I(ALT = 0)  ; Equipo: PANAMA   ||  H.D: - 

~Nodo(ALT = 0):  ; Equipo: PANAMA
 H.I: -  H.D: - 

~Nodo(ALT = 1):  ; Equipo: URUGUAY
 H.I: -   H.D(ALT = 0)  ; Equipo: VENEZUELA

~Nodo(ALT = 0):  ; Equipo: VENEZUELA
 H.I: -  H.D: - 



